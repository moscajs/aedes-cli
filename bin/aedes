#!/usr/bin/env node

var ascii = `
 ______   ________  _______   ________   ______  
/      \\ |        \\|       \\ |        \\ /      \\ 
|  $$$$$$\\| $$$$$$$$| $$$$$$$\\| $$$$$$$$|  $$$$$$\\
| $$__| $$| $$__    | $$  | $$| $$__    | $$___\\$$
| $$    $$| $$  \\   | $$  | $$| $$  \\    \\$$    \\ 
| $$$$$$$$| $$$$$   | $$  | $$| $$$$$    _\\$$$$$$\\
| $$  | $$| $$_____ | $$__/ $$| $$_____ |  \\__| $$
| $$  | $$| $$     \\| $$    $$| $$     \\ \\$$    $$
\\$$   \\$$ \\$$$$$$$$ \\$$$$$$$  \\$$$$$$$$  \\$$$$$$                                        
`

console.log(ascii)

require('../lib/cli')(process.argv)
  .catch((err) => {
    console.log(err.message)
    process.exit(1)
  })
